namespace akira_recog_obj
{
    class value
  {
    double x;
    double y;
    double z;
    
  public:
    value () { x = 0; y = 0; z = 0; }
    ~value () {}
    
    void setAll( double num ) { x = num; y = num; z = num; } 
    void setX ( double num ) { x = num; }
    void setY ( double num ) { y = num; }
    void setZ ( double num ) { z = num; }

  };
  
  class table_filter
  {
  public:
    value max;
    value min;
    table_filter () {}
    ~table_filter () {}
  };
}
